
public class StudentAthlete {
	private Sports sport;
	private int ranking;
	
	public Sports getSport() {
		return sport;
	}
	
	public void setSport(Sports sport) {
		this.sport = sport;
	}
	
	public int getRanking() {
		return ranking;
	}
	
	public void setRanking(int ranking) {
		this.ranking = ranking;
	}
}
